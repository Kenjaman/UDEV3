package fr.epsi.udev.ex10;

import fr.epsi.udev.exo9.Voiture;

public class Moto extends Voiture{

	public Moto() {
		super();
	}

	public Moto(String modele, int anneeMod, double prix, String couleur) {
		super(modele,anneeMod, prix,couleur);	
	}

	public void demarreVehicule() {
		System.out.println("La moto démarre....");
		this.demarrer=true;
	}
	public void arreterVehicule() {
		this.demarrer=false;
		System.out.println("La moto s'arrète....");
	}
}
