package fr.epsi.udev.exo9;

public class MainExo9 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Voiture []voitures = new Voiture[5];
		for(int i=0; i<voitures.length;i++) {
			voitures[i] = new Voiture ("Peugeot 20"+i, 2000+i, 3500+i,"rouge");
			voitures[i].demarreVehicule();
			voitures[i].arreterVehicule();
			
		}
				
	}
	
}
